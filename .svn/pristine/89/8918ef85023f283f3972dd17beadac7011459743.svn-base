	public Json_Devices parseDevices(Devices dev, DeviceGpsLocations devLoc, String orgId) throws SQLException
	{
		if (devLoc != null)
		{
			this.latitude = devLoc.getLatitude();
			this.longitude = devLoc.getLongitude();
		}

		parseDevices(dev, orgId);

		return this;
	}

