package com.littlecloud.control.dao.criteria;

public class NetworkEmailNotificationsCriteria {
	private Integer networkId;
	private String alertType;
	private String level;
	private Boolean enabled;
	private String recipientType;
	public Integer getNetworkId() {
		return networkId;
	}
	public void setNetworkId(Integer networkId) {
		this.networkId = networkId;
	}
	public String getAlertType() {
		return alertType;
	}
	public void setAlertType(String alertType) {
		this.alertType = alertType;
	}
	public String getLevel() {
		return level;
	}
	public void setLevel(String level) {
		this.level = level;
	}
	public Boolean getEnabled() {
		return enabled;
	}
	public void setEnabled(Boolean enabled) {
		this.enabled = enabled;
	}
	public String getRecipientType() {
		return recipientType;
	}
	public void setRecipientType(String recipientType) {
		this.recipientType = recipientType;
	}
	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("NetworkEmailNotificationsCriteria [networkId=");
		builder.append(networkId);
		builder.append(", alertType=");
		builder.append(alertType);
		builder.append(", level=");
		builder.append(level);
		builder.append(", enabled=");
		builder.append(enabled);
		builder.append(", recipientType=");
		builder.append(recipientType);
		builder.append("]");
		return builder.toString();
	}
	
	
}
