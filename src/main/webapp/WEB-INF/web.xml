<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>LittleCloud Web Service</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <display-name>ClientCommands</display-name>
    <servlet-name>ClientCommands</servlet-name>
    <servlet-class>com.littlecloud.control.webservices.ClientCommandsWs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClientCommands</servlet-name>
    <url-pattern>/DataWs/ClientCommands</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>CaptivePortal</display-name>
    <servlet-name>CaptivePortal</servlet-name>
    <servlet-class>com.littlecloud.control.webservices.CaptivePortalWs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CaptivePortal</servlet-name>
    <url-pattern>/DataWs/CaptivePortal</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>GeoFences</display-name>
    <servlet-name>GeoFences</servlet-name>
    <servlet-class>com.littlecloud.control.webservices.GeoFencesWs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GeoFences</servlet-name>
    <url-pattern>/DataWs/GeoFences</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>Config</display-name>
    <servlet-name>Config</servlet-name>
    <servlet-class>com.littlecloud.control.webservices.ConfigWs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Config</servlet-name>
    <url-pattern>/DataWs/Config</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>Device</display-name>
    <servlet-name>Device</servlet-name>
    <servlet-class>com.littlecloud.control.webservices.DeviceWs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Device</servlet-name>
    <url-pattern>/DataWs/Device</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>Network</display-name>
    <servlet-name>Network</servlet-name>
    <servlet-class>com.littlecloud.control.webservices.NetworkWs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Network</servlet-name>
    <url-pattern>/DataWs/Network</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>Client</display-name>
    <servlet-name>Client</servlet-name>
    <servlet-class>com.littlecloud.control.webservices.ClientWs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Client</servlet-name>
    <url-pattern>/DataWs/Client</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>Organization</display-name>
    <servlet-name>Organization</servlet-name>
    <servlet-class>com.littlecloud.control.webservices.OrganizationWs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Organization</servlet-name>
    <url-pattern>/DataWs/Organization</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>Radio</display-name>
    <servlet-name>Radio</servlet-name>
    <servlet-class>com.littlecloud.control.webservices.RadioWs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Radio</servlet-name>
    <url-pattern>/DataWs/Radio</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>Firmware</display-name>
    <servlet-name>Firmware</servlet-name>
    <servlet-class>com.littlecloud.control.webservices.FirmwareWs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Firmware</servlet-name>
    <url-pattern>/DataWs/Firmware</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>Tunnel</display-name>
    <servlet-name>Tunnel</servlet-name>
    <servlet-class>com.littlecloud.control.webservices.TunnelWs</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Tunnel</servlet-name>
    <url-pattern>/DataWs/Tunnel</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ACService</servlet-name>
    <servlet-class>com.littlecloud.ac.ACService</servlet-class>
    <load-on-startup>5</load-on-startup>
  </servlet>
  <filter>
    <filter-name>struts2</filter-name>
    <filter-class>org.apache.struts2.dispatcher.ng.filter.StrutsPrepareAndExecuteFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>struts2</filter-name>
    <url-pattern>/monitor/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>struts2</filter-name>
    <url-pattern>/report/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>struts2</filter-name>
    <url-pattern>/firmware/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>struts2</filter-name>
    <url-pattern>/devconfig/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>DeviceDiagnosticReport</servlet-name>
    <servlet-class>com.littlecloud.control.servlet.DeviceDiagnosticReportServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeviceDiagnosticReport</servlet-name>
    <url-pattern>/servlet/devdiagrpt</url-pattern>
  </servlet-mapping>
</web-app>